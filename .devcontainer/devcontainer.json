{
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".."
    },
    "features": {
        "ghcr.io/devcontainers/features/github-cli:1": {},
        "ghcr.io/devcontainers/features/common-utils:2.5.4": {
            "installSsh": true
        },
        "ghcr.io/gondeknp/devcontainer_jupyterhub_interoperator/jupyter-compat:latest": {}
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-toolsai.jupyter",
                "ms-toolsai.datawrangler"
            ]
        }
    }
}